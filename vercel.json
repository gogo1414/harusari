{
  "crons": [
    {
      "path": "/api/cron/recurring",
      "schedule": "0 15 * * *"
    },
    {
      "path": "/api/push/send?type=morning",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/push/send?type=evening",
      "schedule": "0 12 * * *"
    },
    {
      "path": "/api/push/send?type=monthly",
      "schedule": "0 1 1 * *"
    }
  ]
}
